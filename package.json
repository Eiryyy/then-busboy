{
  "name": "then-busboy",
  "version": "2.0.0",
  "description": "Promise-based wrapper around Busboy, inspired by async-busboy",
  "main": "./main.js",
  "engines": {
    "node": ">=7.6.x"
  },
  "keywords": [
    "multipart",
    "promise",
    "async",
    "formdata"
  ],
  "scripts": {
    "lint": "node_modules/.bin/eslint src",
    "make": "node_modules/.bin/babel src -d .",
    "watch": "node_modules/.bin/babel src -w -d .",
    "make:ci": "node_modules/.bin/babel src -s -d .",
    "m": "yarn make",
    "w": "yarn watch",
    "cleanup": "node_modules/.bin/rimraf lib test",
    "test": "node_modules/.bin/ava",
    "coverage": "node_modules/.bin/nyc npm run test",
    "report": "yarn make:ci && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test && yarn cleanup && yarn make",
    "report:ci": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make:ci && yarn coverage && yarn report:ci && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn make:ci && yarn coverage"
  },
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/**/__hook__/**",
      "!test/**/__helper__/**",
      "!test/helper/**"
    ]
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "@octetstream/invariant": "1.0.1",
    "busboy": "0.2.14",
    "lodash.isboolean": "3.0.3",
    "lodash.isempty": "4.4.0",
    "lodash.isinteger": "4.0.4",
    "lodash.isplainobject": "4.0.6",
    "lodash.merge": "4.6.0",
    "nanoid": "0.2.0",
    "require-dir": "0.3.2",
    "shortid": "2.2.8",
    "type-is": "1.6.15"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "1.0.1",
    "ava": "^0.22.0",
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "co": "4.6.0",
    "codecov": "2.3.0",
    "eslint": "3.19.x",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-ava": "4.2.1",
    "husky": "0.14.3",
    "nyc": "11.1.0",
    "promise-fs": "1.2.3",
    "proxyquire": "1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "supertest": "3.0.0"
  },
  "repository": "git+https://github.com/octet-stream/then-busboy.git",
  "bugs": {
    "url": "https://github.com/octet-stream/then-busboy/issues"
  }
}
