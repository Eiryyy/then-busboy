{
  "name": "then-busboy",
  "version": "1.5.0",
  "description": "Promise-based wrapper around Busboy, inspired by async-busboy",
  "main": "./main.js",
  "engines": {
    "node": ">=7.6.x"
  },
  "keywords": [
    "multipart",
    "promise",
    "async",
    "formdata"
  ],
  "scripts": {
    "lint": "node_modules/.bin/coffeelint src",
    "make": "node_modules/.bin/coffee --no-header -b -o . -c src/",
    "watch": "node_modules/.bin/coffee --no-header -b -o . -cw src/",
    "make:ci": "node_modules/.bin/coffee --no-header -b -o . -cm src/",
    "test": "node_modules/.bin/ava",
    "coverage": "node_modules/.bin/nyc npm run test",
    "report": "yarn make:ci && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:ci": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make:ci && yarn coverage && yarn report:ci && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn make:ci && yarn coverage"
  },
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "test/unit"
    ]
  },
  "dependencies": {
    "busboy": "0.2.14",
    "lodash.isboolean": "3.0.3",
    "lodash.isempty": "4.4.0",
    "lodash.isinteger": "4.0.4",
    "lodash.isplainobject": "4.0.6",
    "lodash.merge": "4.6.0",
    "require-dir": "0.3.2",
    "shortid": "2.2.8",
    "type-is": "1.6.15"
  },
  "devDependencies": {
    "ava": "0.21.0",
    "co": "4.6.0",
    "codecov": "2.2.0",
    "coffee-script": "1.12.7",
    "coffeelint": "1.16.0",
    "gulp-plumber": "1.1.0",
    "gulp-util": "3.0.8",
    "husky": "0.14.3",
    "nyc": "11.0.3",
    "promise-fs": "1.2.3",
    "proxyquire": "1.8.0",
    "sinon": "2.3.8",
    "supertest": "3.0.0"
  },
  "repository": "git+https://github.com/octet-stream/then-busboy.git",
  "bugs": {
    "url": "https://github.com/octet-stream/then-busboy/issues"
  }
}
